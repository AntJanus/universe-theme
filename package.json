  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "VS code",
    "theme",
    "dark theme",
    "universe",
    "universe theme",
    "twitter",
    "night mode",
    "tailwind",
    "tema",
    "tema oscuro",
    "universo"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Universe",

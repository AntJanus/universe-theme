{
  "name": "universe",
  "displayName": "Universe",
  "description": "A dark theme for passionate developers",
  "version": "0.0.2",
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/node": "^11.11.0",
    "deepmerge": "^3.2.0",
    "flat": "^4.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "build": "npm run ts:build && npm run node:build",
    "node:build": "node dist/index.js",
    "node:watch": "nodemon dist/index.js",
    "ts:build": "tsc",
    "ts:watch": "tsc --watch",
    "publish": "vsce publish"
  },
  "publisher": "MatiasOlivera",
  "icon": "static/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatiasOlivera/universe-theme"
